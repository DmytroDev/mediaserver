<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <packaging>jar</packaging>

    <parent>
        <groupId>org.mobicents.media.endpoints</groupId>
        <artifactId>endpoints</artifactId>
        <version>2.4.0-SNAPSHOT</version>
    </parent>

    <groupId>org.mobicents.media.endpoints</groupId>
    <artifactId>packet-relay</artifactId>

    <name>Packet relay</name>
    
    <dependencies>
        <dependency>
            <groupId>org.mobicents.media</groupId>
            <artifactId>spi</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.mobicents.media</groupId>
            <artifactId>components</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.mobicents.servers.media.codecs</groupId>
            <artifactId>g711</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.mobicents.servers.media.codecs</groupId>
            <artifactId>g729</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.mobicents.servers.media.codecs</groupId>
            <artifactId>gsm</artifactId>
            <version>${project.version}</version>
        </dependency>
	<dependency>
            <groupId>org.mobicents.servers.media.codecs</groupId>
            <artifactId>l16</artifactId>
            <version>${project.version}</version>
        </dependency>
	<dependency>
            <groupId>org.mobicents.servers.media.codecs</groupId>
            <artifactId>ilbc</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.mobicents.media.endpoints</groupId>
            <artifactId>conference-bridge</artifactId>
            <version>${project.version}</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.mobicents.media.endpoints</groupId>
            <artifactId>ivr</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.mobicents.media</groupId>
            <artifactId>chassis</artifactId>
            <version>${project.version}</version>
            <type>jar</type>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.5</source>
                    <target>1.5</target>
                </configuration>
            </plugin>
        </plugins>
        <finalName>mobicents-packetrelay-${project.version}</finalName>
    </build>

</project>
