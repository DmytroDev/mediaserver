<MediaServer>
	<Network>
		<BindAddress>127.0.0.1</BindAddress>
		<ExternalAddress>127.0.0.1</ExternalAddress>
		<Network>127.0.0.1</Network>
		<Subnet>255.255.255.255</Subnet>
		<Sbc>false</Sbc>
	</Network>

	<Controller protocol="mgcp">
		<Address>127.0.0.1</Address>
		<Port>2427</Port>
		<Endpoints>
			<Endpoint name="mobicents/bridge" num="50" />
			<Endpoint name="mobicents/ivr" num="50" />
			<Endpoint name="mobicents/cnf" num="50" />
		</Endpoints>
	</Controller>

	<Media>
		<Timeout>0</Timeout>
		<LowPort>34534</LowPort>
		<HighPort>65535</HighPort>
		<JitterBuffer size="50" />
		<codecs>
			<codec name="l16" />
			<codec name="pcmu" />
			<codec name="pcma" />
			<codec name="gsm" />
			<codec name="g729" />
		</codecs>
	</Media>

	<Resources>
		<LocalConnection number="100" />
		<RemoteConnection number="50" />
		<Player number="50" />
		<Recorder number="50" />
		<DtmfDetector number="50" dbi="-35" />
		<DtmfGenerator number="50" toneVolume="-20" toneDuration="80" />
		<SignalDetector number="0" />
		<SignalGenerator number="0" />
	</Resources>
</MediaServer>